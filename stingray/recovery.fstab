# mount point	fstype		device

#external sdcard
/sdcard		vfat		/dev/block/mmcblk0p1		/dev/block/mmcblk0
#internal sdcard
/emmc		emmc 		/devices/platform/sdhci-tegra.2/mmc_host/mmc1
/system		ext4		/dev/block/mmcblk1p8
/cache		ext4		/dev/block/mmcblk1p9
/data		ext4		/dev/block/mmcblk1p10
/misc 		emmc 		/dev/block/mmcblk1p3
/boot		emmc		/dev/block/mmcblk1p7
/recovery	emmc		/dev/block/mmcblk1p6
